{
  "name": "glip-integration-github",
  "version": "0.0.1",
  "description": "glip-integration-github",
  "keywords": [
    "RingCentral",
    "GitHub"
  ],
  "scripts": {
    "ngrok": "ngrok http -region ap 6066",
    "start": "node -r @babel/register -r dotenv/config ./src/server/run-server.js",
    "deploy": "npm run build-lambda && npm run deploy-lambda",
    "fix": "./node_modules/.bin/standard --fix",
    "compile": "babel src/server --out-dir dist/server",
    "c": "./node_modules/.bin/webpack serve --config build/webpack.config.js",
    "build": "npm run clean && npm run compile",
    "view": "node build/view.js",
    "clean": "node build/clean.js",
    "build-lambda": "npm run clean && node cmds/build && npm run view",
    "deploy-lambda": "node cmds/deploy",
    "watch": "cd deploy && ../node_modules/.bin/sls logs -f app -t",
    "lint": "./node_modules/.bin/standard"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.6.2",
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/register": "7.14.5",
    "antd": "^4.10.3",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "css-loader": "5.2.6",
    "css-minimizer-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "less": "^4.1.1",
    "less-loader": "10.0.1",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "0.9.0",
    "ngrok": "^3.3.0",
    "ringcentral-notification-integration-helper": "^0.4.2",
    "serverless": "^2.3.0",
    "serverless-deployment-bucket": "^1.3.0",
    "shelljs": "^0.8.4",
    "shortid": "^2.2.15",
    "standard": "14.3.1",
    "stylus": "^0.54.8",
    "stylus-loader": "6.1.0",
    "url-loader": "4.1.1",
    "webpack": "5.42.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@octokit/core": "^3.1.2",
    "@octokit/oauth-app": "^1.2.0",
    "dynamo-sequelize": "^0.8.1",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^6.0.0",
    "glip-integration-js": "^0.2.0",
    "jsonwebtoken": "^8.5.1",
    "pug": "^3.0.0",
    "sequelize": "^5.22.3",
    "serverless-http": "^2.6.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*-compiled.js",
      "dist",
      "external"
    ]
  }
}
